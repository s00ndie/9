def popular_words (text: str, words: list)-> dict:
    text_low = text.lower().split()
    res = {}
    for word in words:
        res[word] = text_low.count(word)
    return res
print(popular_words('''When I was One I had just begun When I was Two I was nearly new ''', ['i', 'was', 'three', 'near']))
